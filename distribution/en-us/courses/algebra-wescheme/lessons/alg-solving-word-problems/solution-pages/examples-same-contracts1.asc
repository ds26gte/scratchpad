         (* 30 50))
(EXAMPLE (mystery 10)
         (text "Welcome!" 10 "darkgreen"))%ENDCURRICULUMtt%%ENDCURRICULUMdiv%%ENDCURRICULUMdiv%
%CURRICULUMdiv% class="rightColumn"%BEGINCURRICULUMdiv%no match (the range types are different)%ENDCURRICULUMdiv%
