(EXAMPLE (mystery 20)
         (text "New York" 20 "red"))%ENDCURRICULUMtt%%ENDCURRICULUMdiv%%ENDCURRICULUMdiv%
%CURRICULUMdiv% class="rightColumn"%BEGINCURRICULUMdiv%no match (the domain types are different)%ENDCURRICULUMdiv%
